{
  "consumer": {
    "name": "ScriptBee UI"
  },
  "interactions": [
    {
      "description": "a request for script languages",
      "providerStates": [
        {
          "name": "an error while getting script languages"
        }
      ],
      "request": {
        "method": "GET",
        "path": "/api/scripts/languages"
      },
      "response": {
        "body": {
          "code": 500,
          "message": "message"
        },
        "headers": {
          "Content-Type": "application/json"
        },
        "matchingRules": {
          "body": {
            "$.code": {
              "combine": "AND",
              "matchers": [
                {
                  "match": "type"
                }
              ]
            },
            "$.message": {
              "combine": "AND",
              "matchers": [
                {
                  "match": "type"
                }
              ]
            }
          },
          "header": {}
        },
        "status": 500
      }
    },
    {
      "description": "a request for script languages",
      "providerStates": [
        {
          "name": "existing script languages"
        }
      ],
      "request": {
        "method": "GET",
        "path": "/api/scripts/languages"
      },
      "response": {
        "body": [
          {
            "extension": "js",
            "name": "JavaScript"
          }
        ],
        "headers": {
          "Content-Type": "application/json"
        },
        "matchingRules": {
          "body": {
            "$": {
              "combine": "AND",
              "matchers": [
                {
                  "match": "type",
                  "min": 1
                }
              ]
            },
            "$[*].extension": {
              "combine": "AND",
              "matchers": [
                {
                  "match": "type"
                }
              ]
            },
            "$[*].name": {
              "combine": "AND",
              "matchers": [
                {
                  "match": "type"
                }
              ]
            }
          },
          "header": {}
        },
        "status": 200
      }
    }
  ],
  "metadata": {
    "pact-js": {
      "version": "10.4.1"
    },
    "pactRust": {
      "ffi": "0.4.0",
      "models": "1.0.4"
    },
    "pactSpecification": {
      "version": "3.0.0"
    }
  },
  "provider": {
    "name": "ScriptBee API"
  }
}