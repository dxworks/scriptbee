{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Debug",
        "System": "Debug",
        "Microsoft.Hosting.Lifetime": "Debug"
      }
    },
    "Enrich": [
      "FromLogContext"
    ],
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "formatter": {
            "type": "Serilog.Templates.ExpressionTemplate, Serilog.Expressions",
            "template": "[{UtcDateTime(@t):o}][{@l:u3}][{Coalesce(SourceContext, undefined())}{Coalesce(Concat('::', MethodName), undefined())}{Coalesce(Concat('&L', LineNumber), undefined())}] {@m}\n{@x}",
            "theme": "Serilog.Templates.Themes.TemplateTheme::Literate, Serilog.Expressions"
          }
        }
      }
    ]
  },
  "Jwt": {
    "Authority": "http://localhost:28080/realms/scriptbee",
    "AuthorizationUrl": "http://localhost:28080/realms/scriptbee/protocol/openid-connect/auth",
    "TokenUrl": "http://localhost:28080/realms/scriptbee/protocol/openid-connect/token",
    "Audience": "account",
    "Realm": "scriptbee",
    "ClientId": "scriptbee-backend-client",
    "ClientSecret": "h4ZNdwcnGKow8kxKsyexPKtMdU7rx8fp"
  }
}
